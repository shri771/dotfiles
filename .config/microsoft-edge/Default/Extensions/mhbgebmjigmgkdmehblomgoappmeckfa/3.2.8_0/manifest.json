{
   "action": {
      "default_title": "Notion Saver"
   },
   "background": {
      "service_worker": "serviceWorker.js"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Alt+Shift+E",
            "mac": "Alt+Shift+E"
         }
      },
      "add-highlights": {
         "description": "Add highlights to your Notion Page",
         "suggested_key": {
            "default": "Ctrl+Shift+E",
            "mac": "Command+Shift+E"
         }
      },
      "take-custom-area-screenshot": {
         "description": "Take custom area screenshot",
         "suggested_key": {
            "default": "Ctrl+Shift+A",
            "mac": "Command+Shift+A"
         }
      },
      "take-full-page-screenshot": {
         "description": "Take full page screenshot",
         "suggested_key": {
            "default": "Ctrl+Shift+S",
            "mac": "Command+Shift+S"
         }
      }
   },
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
   },
   "description": "Save the Web to Notion",
   "host_permissions": [ "https://*.notion.so/*" ],
   "icons": {
      "128": "./assets/icon128.png",
      "48": "./assets/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Save to Notion",
   "optional_host_permissions": [ "https://*.licdn.com/*", "\u003Call_urls>" ],
   "optional_permissions": [ "tabs", "webNavigation" ],
   "permissions": [ "activeTab", "storage", "cookies", "scripting", "contextMenus", "unlimitedStorage" ],
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "3.2.8",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "popup/index.html", "modal/modal.html", "toast/toast.html", "assets/*.js" ]
   } ]
}
