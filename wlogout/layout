{
    "label" : "lock",
    "action" : "$HOME/.config/hypr/scripts/LockScreen.sh",
    "text" : "Lock",
    "keybind" : "l"
}
{
    "label" : "reboot",
    "action" : "systemctl reboot",
    "text" : "Reboot",
    "keybind" : "r"
}
{
    "label" : "shutdown",
    "action" : "systemctl poweroff",
    "text" : "Shutdown",
    "keybind" : "p"
}
{
    "label" : "logout",

    "action" : "awesome-client 'awesome.quit()' && loginctl kill-session $XDG_SESSION_ID",
    "text" : "Logout",
    "keybind" : "e"
}
{
    "label" : "suspend",
    "action" : "systemctl suspend && $HOME/.config/hypr/scripts/LockScreen.sh",
    "text" : "Suspend",
    "keybind" : "s"
}
{
    "label" : "hibernate",
    "action" : "systemctl hibernate",
    "text" : "Hibernate",
    "keybind" : "h"
}
